{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "declaration": false,
    "lib": [
      "ES2015",
      "DOM"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@classes/*": [
        "./src/classes/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@config/*": [
        "./src/config/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
